{
  "name": "lilnouns-ens",
  "version": "1.0.0-alpha.9",
  "private": true,
  "description": "LilNouns ENS domain mapping service with upgradeable smart contracts",
  "keywords": [],
  "license": "Apache-2.0",
  "author": "Milad Nekofar <milad@nekofar.com>",
  "scripts": {
    "build": "turbo run build",
    "build:contracts": "turbo run build --filter=@lilnouns-ens/contracts",
    "coverage": "turbo run coverage",
    "coverage:contracts": "turbo run coverage --filter=@lilnouns-ens/contracts",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md,sol}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,sol}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "analyze:sizes": "bash scripts/analyze-sizes.sh"
  },
  "resolutions": {
    "base-x": ">=3.0.11",
    "brace-expansion": ">=2.0.2",
    "cookie": ">=0.7.0",
    "debug": ">=3.2.7",
    "elliptic": ">=6.6.1",
    "eth-gas-reporter": ">=0.2.26",
    "flat": ">=5.0.1",
    "minimatch": ">=3.0.5",
    "pbkdf2": ">=3.1.3",
    "request": ">=2.88.2",
    "tough-cookie": ">=4.1.3",
    "word-wrap": ">=1.2.4",
    "ws": ">=7.5.10"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-solidity": "2.1.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "prettier-plugin-toml": "2.0.6",
    "turbo": "2.5.6"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "pnpm": ">=10.0.0"
  }
}
